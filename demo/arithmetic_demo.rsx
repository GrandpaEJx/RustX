// Arithmetic operations demonstration
println("=== Arithmetic Operations ===")

Int x = 10
Int y = 5

println("x = 10, y = 5")
print("x + y = ")
println(x + y)

print("x - y = ")
println(x - y)

print("x * y = ")
println(x * y)

print("x / y = ")
println(x / y)

Float a = 10.5
Float b = 2.5

println("\nFloat operations:")
printf("a = {}, b = {}\n", a, b)
printf("a + b = {}\n", a + b)
printf("a * b = {}\n", a * b)

Bool result = x == y
printf("\nComparison: x == y is {}\n", result)